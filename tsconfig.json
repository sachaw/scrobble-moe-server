{
  "compilerOptions": {
    "outDir": "dist",
    "strict": true,
    "pretty": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "removeComments": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "paths": {
      "@protobufs/*": ["./src/generated/moe/scrobble/*"]
    }
  },
  "include": ["src/**/*"]
}
